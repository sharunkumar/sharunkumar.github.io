{
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": true
    },
    "color-highlight.enable": true,
    "color-highlight.matchRgbWithNoFunction": true,
    "[json]": {
        "editor.formatOnSave": true,
    },
}